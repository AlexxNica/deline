  "homepage": "https://github.com/airbnb/deline#readme",
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "babel-preset-airbnb": "^2.1.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "jest-cli": "^16.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
